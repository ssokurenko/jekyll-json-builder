---
layout: null
---
{
  {% for post in site.posts limit:10 %}
  "{{ forloop.index }}" : {
    "title" : "{{ post.title }}",
    "date" : "{{ post.date }}",
    "description" : "{{ post.description }}",
    "categories" : [
    {% for category in post.categories%}
      "{{ category }}"
      {% if forloop.index < forloop.length %}
      ,
    {% endif %}
    {% endfor%}
    
    ]
  }
   {% if forloop.index < forloop.length %}
   ,
   {% endif %}
  {% endfor %}
}